<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/assets/css/bootstrap.min.css")
    @Styles.Render("~/assets/css/bootstrap-responsive.min.css")
    @Styles.Render("~/assets/css/tablecloth.css")
    @Styles.Render("~/assets/css/prettify.css")
    @Scripts.Render("~/bundles/modernizr")
    
    <style type="text/css">
		body { padding-top: 60px; padding-bottom: 40px; }
		.box {border: thin black solid; background-color: #EEEEEE; min-height: 50px; margin-bottom: 15px; padding-bottom: 5px; padding-top: 5px;}
	</style>
</head>
<body>
        <div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
		    <div class="container">
		        @Html.ActionLink("Scholarship Admin", "Index", "Home",null, new {@class = "brand centered-content"})
		    </div>
		</div>
	</div>

	<div class="container">
        @RenderBody()
	  	<hr>

		<footer>
			<p>&copy; AFS USA</p>
		</footer>
	</div>

        
        @Scripts.Render("~/assets/js/jquery-1.7.2.min.js")
        @Scripts.Render("~/assets/js/bootstrap.min.js")
        @Scripts.Render("~/assets/js/jquery.metadata.js")
        @Scripts.Render("~/assets/js/jquery.tablecloth.js")
        @Scripts.Render("~/assets/js/jquery.tablesorter.min.js")
        @RenderSection("scripts", required: false)
    </body>
</html>
